// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int    @id @default(autoincrement())
  firstName String
  lastName  String
  age       Int
}

model Test {
  id   Int    @id @default(autoincrement())
  name String
  age  Int
}

model Pet {
  id         String  @id @default(uuid())
  name       String
  tutorName  String
  age        Int
  species    String
}
